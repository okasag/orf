set.seed(123)
orf_fit <- orf(X,Y)
# predict ordered probabilities
orf_pred <- predict(orf_fit)$predictions
# compute MSE (in-sample)
orf_mse <- mean((rowSums((real_prob - orf_pred)^2)/3))
