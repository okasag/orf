X <- as.matrix(cbind(X1, X2, X3, X4))
# generate some data
# coefficients
betas <- c(1, 1, 1, 0)
# deterministic component
deterministic <- 1*(X1) + 1*(X2) + 1*(X3)
# generate outcome with logistic error
